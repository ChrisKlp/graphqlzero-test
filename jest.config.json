{
  "preset": "ts-jest",
  "testEnvironment": "jestdom",
  "testMatch": ["**/__tests__/**/*.ts?(x)", "**/?(*.)+(test).ts?(x)"],
  "moduleDirectories": ["node_modules", "src"],
  "transform": {
    "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
  },
  "transformIgnorePatterns": ["node_modules/(?!variables/.*)"]
}
